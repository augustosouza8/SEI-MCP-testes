{
  "name": "sei-mcp",
  "version": "1.0.0",
  "description": "MCP server + Chrome extension para automação do sistema SEI!",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "sei-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts src/http-server.ts src/remote-proxy.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts src/http-server.ts --format esm --watch",
    "start": "node dist/index.js",
    "start:http": "node dist/index.js http",
    "start:stdio": "node dist/index.js",
    "http": "node dist/index.js http",
    "lint": "eslint src --ext .ts",
    "test": "vitest"
  },
  "keywords": [
    "mcp",
    "sei",
    "automation",
    "chrome-extension",
    "governo"
  ],
  "author": "Nicholas Jacob",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.8.0",
    "sei-playwright": "file:vendor/sei-playwright",
    "ws": "^8.18.0",
    "zod": "^3.24.0",
    "zod-to-json-schema": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "optionalDependencies": {
    "@anthropic-ai/sdk": "^0.39.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
