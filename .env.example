# ===== SEI-MCP (HTTP) =====
SEI_MCP_DRIVER=playwright
SEI_MCP_HTTP_PORT=3100
SEI_MCP_PUBLIC_BASE_URL=http://localhost:3100
SEI_MCP_REQUIRE_AUTH=true

# Auth (escolha 1: token estático para o cliente MCP)
SEI_MCP_BEARER_TOKEN=change-me

# Auth (escolha 1b: token do licensing para clientes MCP)
# Se o Bearer token não bater com SEI_MCP_BEARER_TOKEN, o servidor valida o token no licensing e registra uso.
SEI_MCP_LICENSING_API_URL=https://sei-tribunais-licensing-api.onrender.com/api/v1

# Auth (escolha 2: Google OAuth para /pricing)
SEI_MCP_JWT_SECRET=change-me
SEI_MCP_GOOGLE_CLIENT_ID=change-me
SEI_MCP_GOOGLE_CLIENT_SECRET=change-me

# Stripe (para /pricing checkout/portal)
SEI_MCP_STRIPE_SECRET_KEY=sk_test_change_me
SEI_MCP_STRIPE_PRICE_STARTER_MONTHLY=price_change_me
SEI_MCP_STRIPE_PRICE_PRO_MONTHLY=price_change_me

# Playwright
SEI_MCP_PW_HEADLESS=1
SEI_MCP_PW_PERSISTENT=1
# SEI_MCP_PW_CHANNEL=chrome
